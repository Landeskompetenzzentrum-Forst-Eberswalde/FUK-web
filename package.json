		"@tailwindcss/vite": "^4.0.0",
		"@vite-pwa/sveltekit": "^0.6.7",
		"echarts": "^5.6.0",
		"echarts-gl": "^2.0.9",
		"prettier": "^3.4.2",
		"prettier-plugin-svelte": "^3.3.3",
		"smui-theme": "^8.0.0-beta.3",
		"svelte": "^5.0.0",
		"tailwindcss": "^4.0.0",
		"vite": "^6.0.0"
	}
